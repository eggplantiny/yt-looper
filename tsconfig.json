{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "src/",
    "paths": {
      "@/*": [
        "*"
      ]
    }
  },
  "include": [
    "@types/youtube",
    "@types/youtube-player"
  ],
  "exclude": [
    "node_modules"
  ]
}
